---
import Layout from "./Layout.astro";
import Hero from "../components/Hero.astro";

const { title, backLinkTitle, displayBacklink, contentClass } = Astro.props;
---

<Layout title={title} contentClass={contentClass}>
	<Hero
		title={title}
		displayBacklink={displayBacklink}
		backLinkTitle={backLinkTitle}
		slot="after-header"
	/>
	<slot />
</Layout>
