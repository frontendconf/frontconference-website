{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "entry._image.astro"
		},
		{
			"src": "^\\/$",
			"dest": "entry.index.astro"
		},
		{
			"src": "^\\/speakers\\/?$",
			"dest": "entry.speakers.astro"
		},
		{
			"src": "^\\/speakers\\/([^/]+?)\\/?$",
			"dest": "entry._speaker_.astro"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "entry.404.astro"
		}
	]
}