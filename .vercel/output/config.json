{
	"version": 3,
	"routes": [
		{
			"src": "^/_astro/(.*)$",
			"headers": {
				"cache-control": "public, max-age=31536000, immutable"
			},
			"continue": true
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "^\\/_image$",
			"dest": "_image.astro"
		},
		{
			"src": "^\\/$",
			"dest": "index.astro"
		},
		{
			"src": "^\\/speakers\\/?$",
			"dest": "speakers/index.astro"
		},
		{
			"src": "^\\/speakers\\/([^/]+?)\\/?$",
			"dest": "speakers/[speaker].astro"
		},
		{
			"src": "^\\/404\\/?$",
			"dest": "404.astro"
		}
	]
}